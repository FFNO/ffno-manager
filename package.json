{
  "name": "infinity-fe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --port=8000",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "preview": "vite preview",
    "libs:build": "meta git update && meta git checkout master --include-only src/libs && meta git pull --include-only src/libs",
    "knip": "knip",
    "postinstall": "npm run libs:build"
  },
  "dependencies": {
    "@mantine/carousel": "^7.8.0",
    "@mantine/core": "^7.8.0",
    "@mantine/dates": "^7.8.0",
    "@mantine/dropzone": "^7.8.0",
    "@mantine/form": "^7.8.0",
    "@mantine/hooks": "^7.8.0",
    "@mantine/modals": "^7.8.0",
    "@mantine/notifications": "^7.6.1",
    "@nextui-org/react": "^2.3.6",
    "@supabase/supabase-js": "^2.42.0",
    "@tanstack/react-query": "^5.25.0",
    "@tanstack/react-router": "^1.19.4",
    "axios": "^1.6.7",
    "dayjs": "^1.11.10",
    "embla-carousel-react": "^7.1.0",
    "framer-motion": "^11.1.9",
    "hugeicons-react": "^0.2.0",
    "jotai": "^2.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-onesignal": "^3.0.1",
    "socket.io-client": "^4.7.5",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.20.1",
    "@tanstack/router-vite-plugin": "^1.19.6",
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "knip": "^5.15.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.1.4"
